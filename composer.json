{
  "name": "valentine94/vmgen-php",
  "description": "PHP VM Generator tool",
  "license": "MIT",
  "version": "1.1.1",
  "authors": [
    { "name": "Valentine Matsveiko", "email": "mve@drupalway.net" }
  ],
  "bin": [
    "vmgen-php"
  ],
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Vmgen\\": "src/"
